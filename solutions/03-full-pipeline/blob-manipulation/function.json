{
  "scriptFile": "blob_manipulation.py",
  "bindings": [
    {
      "name": "myblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "functionblob/{name}.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "sendGrid",
      "direction": "out",
      "name": "sendemail",
      "apiKey": "SendGridAPIKeyAsAppSetting"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputBlob",
      "path": "functionblob/{name}_tag_plot.png",
      "connection": "AzureWebJobsStorage"
    }
  ]
}